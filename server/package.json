{
	"name": "server",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"license": "MIT",
	"main": "dist/main.js",
	"engines": {
		"node": "10"
	},
	"scripts": {
		"set:config": "firebase functions:config:set schedule.scan_check_status=\"*/1 * * * *\" schedule.screen_shot=\"*/10 * * * *\" schedule.ip_watch=\"*/1 * * * *\" ",
		"build": "webpack",
		"serve": "npm run set:config && webpack && firebase serve --only functions",
		"serve:dev": "nodemon --config \"nodemon-debug.json\" -e ts --exec npm run serve",
		"shell": "webpack && firebase functions:shell",
		"deploy": "npm run set:config && webpack && firebase deploy --only functions,firestore",
		"lint": "tslint -p tsconfig.json -c tslint.json",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@google-cloud/language": "^4.2.0",
		"@google-cloud/vision": "^2.1.2",
		"@nestjs/common": "^6.11.11",
		"@nestjs/core": "^6.11.11",
		"@nestjs/passport": "^7.1.0",
		"@nestjs/platform-express": "^6.11.11",
		"@nestjs/swagger": "^4.7.3",
		"arraybuffer-to-string": "^1.0.2",
		"express": "^4.17.1",
		"firebase-admin": "^9.4.1",
		"firebase-functions": "^3.11.0",
		"helmet": "^4.2.0",
		"image-to-base64": "^2.1.1",
		"md5": "^2.2.1",
		"moment-timezone": "^0.5.27",
		"passport": "^0.4.1",
		"passport-headerapikey": "^1.2.1",
		"reflect-metadata": "^0.1.12",
		"rimraf": "^2.6.2",
		"rollbar": "^2.14.4",
		"rxjs": "^6.3.3",
		"shodan-client": "^3.1.5",
		"stripe": "^8.109.0",
		"swagger-ui-express": "^4.1.4",
		"uuid": "^8.3.0"
	},
	"devDependencies": {
		"@nestjs/testing": "^6.11.11",
		"@types/express": "^4.16.0",
		"@types/jest": "^23.3.13",
		"@types/md5": "^2.1.33",
		"@types/moment-timezone": "^0.5.12",
		"@types/node": "^10.17.28",
		"@types/passport": "^1.0.4",
		"@types/supertest": "^2.0.7",
		"@types/uuid": "^8.0.0",
		"concurrently": "^4.1.0",
		"jest": "^23.6.0",
		"nodemon": "^1.19.4",
		"prettier": "^1.15.3",
		"supertest": "^3.4.1",
		"ts-jest": "^24.2.0",
		"ts-loader": "^6.2.1",
		"ts-node": "^8.5.4",
		"tsconfig-paths": "3.8.0",
		"tslint": "5.16.0",
		"typescript": "3.7.4",
		"wait-on": "^3.2.0",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10",
		"webpack-node-externals": "^1.7.2"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".spec.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	}
}
